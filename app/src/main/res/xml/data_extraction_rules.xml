<?xml version="1.0" encoding="utf-8"?><!--
   Data extraction rules file - control backup behavior for Android 12+
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude DataStore preferences to prevent tutorial state from persisting across installs -->
        <exclude domain="file" path="datastore/"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude DataStore during device transfer as well -->
        <exclude domain="file" path="datastore/"/>
    </device-transfer>
</data-extraction-rules>